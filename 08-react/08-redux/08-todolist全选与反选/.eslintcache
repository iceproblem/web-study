   let lastTodoId = todos.length === 0 ? 0 : todos[todos.length-1].id;\r\n\r\n        if(e.keyCode === 13){\r\n            let content = this.inputRef.current.value;\r\n            if(!content.trim()){\r\n                alert(\"请在输入框中输入内容\");\r\n                return;\r\n            }\r\n            let todo = {id:lastTodoId+1, content ,done:false};\r\n            store.dispatch(addOneTodoAction(todo));\r\n            this.inputRef.current.value = \"\";\r\n        }\r\n    }\r\n    render() {\r\n        return (\r\n            <div className=\"todo-header\">\r\n                <input\r\n                    type=\"text\"\r\n                    ref={ this.inputRef }\r\n                    placeholder=\"请输入今天的任务清单，按回车键确认\"\r\n                    onKeyDown={ e=>this._dealKeyDown(e)}\r\n                />\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\n\r\n\r\n","C:\\AAAAA\\document\\web-study\\08-react\\08-redux\\redux-app\\src\\components\\Todo.jsx",[],"C:\\AAAAA\\document\\web-study\\08-react\\08-redux\\redux-app\\src\\store\\index.js",[],"C:\\AAAAA\\document\\web-study\\08-react\\08-redux\\redux-app\\src\\store\\reducers\\index.js",[],"C:\\AAAAA\\document\\web-study\\08-react\\08-redux\\redux-app\\src\\store\\contains\\types.js",[],"C:\\AAAAA\\document\\web-study\\08-react\\08-redux\\redux-app\\src\\store\\actions\\todos.js",[],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":12,"column":9,"nodeType":"55","endLine":12,"endColumn":13},{"ruleId":"53","severity":1,"message":"54","line":17,"column":9,"nodeType":"55","endLine":17,"endColumn":13},{"ruleId":"53","severity":1,"message":"54","line":14,"column":9,"nodeType":"55","endLine":14,"endColumn":13},"no-native-reassign",["56"],"no-negated-in-lhs",["57"],"react/no-direct-mutation-state","Do not mutate state directly. Use setState().","ThisExpression","no-global-assign","no-unsafe-negation"]